<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/black"
    tools:context=".activity.Pofile.InsuranceAndVehicleInfo">
    <RelativeLayout
        style="@style/Heddersiz"
        android:id="@+id/hedder">

        <ImageView
            style="@style/BackButtonImage"
            android:id="@+id/iv_backArrow_insurance"
            android:onClick="iv_backArrow_insurance"
            android:src="@drawable/backbtn"
            android:tint="@color/White"/>

        <TextView
            style="@style/Hedder"
            android:text="Information">
        </TextView>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/hedder">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/_20sdp">


  <TextView
    style="@style/ScreenTextView2"
      android:textColor="@color/White"
      android:text="Documents">
  </TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginLeft="@dimen/_25sdp"
        android:layout_marginRight="@dimen/_25sdp"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_5sdp">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/_10sdp"
            android:src="@drawable/id_back">
        </ImageView>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10sdp"
            android:src="@drawable/id_front">

        </ImageView>
    </LinearLayout>

        <TextView
            style="@style/ScreenTextView2"
              android:textColor="@color/White"
                android:layout_marginTop="@dimen/_10sdp"
              android:text="Vehicle">

  </TextView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginLeft="@dimen/_25sdp"
        android:layout_marginRight="@dimen/_25sdp"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_5sdp">
        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/_10sdp"
            android:src="@drawable/id_back">
        </ImageView>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"

            android:layout_marginLeft="@dimen/_10sdp"
            android:src="@drawable/id_front">

        </ImageView>
    </LinearLayout>
        <TextView
        style="@style/ScreenTextView2"
         android:textColor="@color/White"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Insurance">

  </TextView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginLeft="@dimen/_25sdp"
        android:layout_marginRight="@dimen/_25sdp"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_5sdp">
        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/_10sdp"
            android:src="@drawable/id_back">
        </ImageView>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="@dimen/_100sdp"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10sdp"
            android:src="@drawable/id_front">

        </ImageView>
    </LinearLayout>
    </LinearLayout>
    </ScrollView>
</RelativeLayout>